---
import Layout from "../layouts/Layout.astro";

const pageTitle = "Home";
---

<Layout title={pageTitle}>
  <main class="flex-col">
    <h1 class="text-4xl font-bold text-center my-8">{pageTitle}</h1>
    <div class="max-w-md px-6 mx-auto">
      <img src="home.svg" />
      <div class="text-2xl font-serif font-thin text-center mt-6">
        work in progress
      </div>
    </div>
  </main>
</Layout>
