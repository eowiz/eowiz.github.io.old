---
import Layout from "../layouts/Layout.astro";
import Navigation from "../components/Navigation.astro";

const pageTitle = "Home";
---

<Layout title={pageTitle}>
  <main class="flex-col mx-6">
    <Navigation />
    <h1 class="text-4xl font-bold text-center my-8">{pageTitle}</h1>
    <img src="home.svg" />
    <div class="text-2xl font-serif font-thin text-center mt-6">
      work in progress
    </div>
  </main>
</Layout>
